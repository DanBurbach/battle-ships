          </tr>
        </tbody>
    </table>
    <input type="submit" class="button" value="Submit">
  </form>
  </div>


</div>
<% temp_y = 1 %>
<% temp_x = 1 %>
  <% while temp_y < 11 do %>
    <% while temp_x < 11 do %>
    <p><%=temp_y%>|<%=temp_x%>, <% temp_x = temp_x + 1 %><%end%><%temp_y = temp_y + 1 %></p><hr>
  <% temp_x = 1 %>
<%end%>

</body>
</html>
