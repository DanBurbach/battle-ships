      </thead>
        <tbody>
          <tr>
            <th class="row-dark" scope="row">1</th>
            <td><div id="1"><input type="checkbox" class="form-control" value="1"><span class="checkmark"></span></div></td>
            <td><div id="2"><input type="checkbox" class="form-control" value="1"><span class="checkmark"></span></div></td>
            <td><div id="3"><input type="checkbox" class="form-control" value="1"><span class="checkmark"></span></div></td>
          </tr>
          <tr>
            <th class="row-dark" scope="row">2</th>
            <td><div id="4"><input type="checkbox" class="form-control" value="1"><span class="checkmark"></span></div></td>
            <td><div id="5"><input type="checkbox" class="form-control" value="1"><span class="checkmark"></span></div></td>
            <td><div id="6"><input type="checkbox" class="form-control" value="1"><span class="checkmark"></span></div></td>
          </tr>
          <tr>
            <th class="row-dark" scope="row">3</th>
